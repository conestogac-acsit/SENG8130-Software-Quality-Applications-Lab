{
  "name": "sqate-tools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_DEV_SERVER=true concurrently \"vite\" \"npm run electron:dev\"",
    "electron:dev": "wait-on http://localhost:5173 && electron electron/public/main.cjs",
    "build": "vite build",
    "start": "npm run dev",
    "start:prod": "npm run build && electron electron/public/main.cjs",
    "preview": "vite preview",
    "test": "jest",
    "package": "electron-packager . SQATE-Tools --platform=win32 --arch=x64 --out=out --overwrite",
    "package:mac": "electron-packager . SQATE-Tools --platform=darwin --arch=x64 --out=out --overwrite",
    "package:linux": "electron-packager . SQATE-Tools --platform=linux --arch=x64 --out=out --overwrite",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "wait-on": "^7.0.1"
  }
}
